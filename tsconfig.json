{
  "compilerOptions": {
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "newLine": "LF",
    "moduleResolution": "node",
    "strict": false,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "outDir": "./dist",
    "module": "esnext",
    "types": [
      "vite/client",
      "node"
    ]
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
